div.ylanavi(sticky, className="ylanavi-sticky")
  div.row
    div.col-xs-8
      ol.breadcrumb
        li
          a(ui-sref="root")
            span.glyphicon.glyphicon-home
        li
          a(ui-sref="root.koulutustoimija") koulutustoimija
        li
          a(href="") 42
        li
          a(href="") ops
        li
          a(href="") 1

    div.col-xs-4
      div.pull-right
        span.dropdown.kielivalitsin
          span(kaanna="'sisalto-kieli'")
          span=": "
          span
            a.action-link.dropdown-toggle(data-toggle="dropdown") fi
              span.caret
            ul.dropdown-menu.dropdown-menu-right
              li(ng-repeat="kieli in langs")
                a(href="" kaanna="kieli")

        span.ohje.popover-element.fa-stack.fa-lg(
          style="color: #199dd0;"
          ng-show="helpUrl"
          uib-popover-template="helpUrl"
          popover-trigger="mouseenter"
          popover-animation="true"
          popover-placement="bottom"
          popover-title="{{ 'apuotsikko' | kaanna }}")
          i.fa.fa-circle.fa-stack-2x
          i.fa.fa-info.fa-stack-1x.fa-inverse
